#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vitest@3.1.2_@types+node@22.15.3_happy-dom@17.4.6_jiti@2.4.2_jsdom@19.0.0_less@4.3.0_sa_971eb0259baa0c84567190ce36d71dc2/node_modules/vitest/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vitest@3.1.2_@types+node@22.15.3_happy-dom@17.4.6_jiti@2.4.2_jsdom@19.0.0_less@4.3.0_sa_971eb0259baa0c84567190ce36d71dc2/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vitest@3.1.2_@types+node@22.15.3_happy-dom@17.4.6_jiti@2.4.2_jsdom@19.0.0_less@4.3.0_sa_971eb0259baa0c84567190ce36d71dc2/node_modules/vitest/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vitest@3.1.2_@types+node@22.15.3_happy-dom@17.4.6_jiti@2.4.2_jsdom@19.0.0_less@4.3.0_sa_971eb0259baa0c84567190ce36d71dc2/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
