#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/scripts/turbo-run/bin/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/scripts/turbo-run/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/scripts/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules:/Users/xiongtianping/Desktop/workportal/node_modules:/Users/xiongtianping/Desktop/node_modules:/Users/xiongtianping/node_modules:/Users/node_modules:/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/scripts/turbo-run/bin/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/scripts/turbo-run/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/scripts/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules:/Users/xiongtianping/Desktop/workportal/node_modules:/Users/xiongtianping/Desktop/node_modules:/Users/xiongtianping/node_modules:/Users/node_modules:/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vben/turbo-run/bin/turbo-run.mjs" "$@"
else
  exec node  "$basedir/../@vben/turbo-run/bin/turbo-run.mjs" "$@"
fi
