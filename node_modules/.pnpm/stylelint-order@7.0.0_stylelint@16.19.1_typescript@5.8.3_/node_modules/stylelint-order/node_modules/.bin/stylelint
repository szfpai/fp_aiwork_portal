#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/stylelint@16.19.1_typescript@5.8.3/node_modules/stylelint/bin/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/stylelint@16.19.1_typescript@5.8.3/node_modules/stylelint/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/stylelint@16.19.1_typescript@5.8.3/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/stylelint@16.19.1_typescript@5.8.3/node_modules/stylelint/bin/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/stylelint@16.19.1_typescript@5.8.3/node_modules/stylelint/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/stylelint@16.19.1_typescript@5.8.3/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stylelint@16.19.1_typescript@5.8.3/node_modules/stylelint/bin/stylelint.mjs" "$@"
else
  exec node  "$basedir/../../../../../stylelint@16.19.1_typescript@5.8.3/node_modules/stylelint/bin/stylelint.mjs" "$@"
fi
