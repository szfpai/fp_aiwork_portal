#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.23.4_@types+node@22.15.3_async-validator@4.2.5_e48b1a75a9825deaf2630d17280bbfb3/node_modules/vitepress/bin/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.23.4_@types+node@22.15.3_async-validator@4.2.5_e48b1a75a9825deaf2630d17280bbfb3/node_modules/vitepress/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.23.4_@types+node@22.15.3_async-validator@4.2.5_e48b1a75a9825deaf2630d17280bbfb3/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.23.4_@types+node@22.15.3_async-validator@4.2.5_e48b1a75a9825deaf2630d17280bbfb3/node_modules/vitepress/bin/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.23.4_@types+node@22.15.3_async-validator@4.2.5_e48b1a75a9825deaf2630d17280bbfb3/node_modules/vitepress/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.23.4_@types+node@22.15.3_async-validator@4.2.5_e48b1a75a9825deaf2630d17280bbfb3/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitepress@1.6.3_@algolia+client-search@5.23.4_@types+node@22.15.3_async-validator@4.2.5_e48b1a75a9825deaf2630d17280bbfb3/node_modules/vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../../../../../vitepress@1.6.3_@algolia+client-search@5.23.4_@types+node@22.15.3_async-validator@4.2.5_e48b1a75a9825deaf2630d17280bbfb3/node_modules/vitepress/bin/vitepress.js" "$@"
fi
