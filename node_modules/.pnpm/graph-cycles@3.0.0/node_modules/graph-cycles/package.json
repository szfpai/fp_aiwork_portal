  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.4.1",
    "rimraf": "^4.1.2",
    "ts-jest-resolver": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
