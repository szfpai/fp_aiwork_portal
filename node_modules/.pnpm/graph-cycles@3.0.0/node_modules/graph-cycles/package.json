{
  "name": "graph-cycles",
  "version": "3.0.0",
  "description": "Analyze a graph to find cyclic loops",
  "author": "Gustaf Räntilä",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grantila/graph-cycles/issues"
  },
  "homepage": "https://github.com/grantila/graph-cycles#readme",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
