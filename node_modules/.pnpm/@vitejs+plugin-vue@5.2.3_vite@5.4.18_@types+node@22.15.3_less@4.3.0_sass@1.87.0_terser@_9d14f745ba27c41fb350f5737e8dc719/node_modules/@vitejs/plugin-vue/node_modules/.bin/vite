#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vite@5.4.18_@types+node@22.15.3_less@4.3.0_sass@1.87.0_terser@5.39.0/node_modules/vite/bin/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vite@5.4.18_@types+node@22.15.3_less@4.3.0_sass@1.87.0_terser@5.39.0/node_modules/vite/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vite@5.4.18_@types+node@22.15.3_less@4.3.0_sass@1.87.0_terser@5.39.0/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vite@5.4.18_@types+node@22.15.3_less@4.3.0_sass@1.87.0_terser@5.39.0/node_modules/vite/bin/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vite@5.4.18_@types+node@22.15.3_less@4.3.0_sass@1.87.0_terser@5.39.0/node_modules/vite/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/vite@5.4.18_@types+node@22.15.3_less@4.3.0_sass@1.87.0_terser@5.39.0/node_modules:/Users/xiongtianping/Desktop/workportal/fp_aiwork_portal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@5.4.18_@types+node@22.15.3_less@4.3.0_sass@1.87.0_terser@5.39.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@5.4.18_@types+node@22.15.3_less@4.3.0_sass@1.87.0_terser@5.39.0/node_modules/vite/bin/vite.js" "$@"
fi
